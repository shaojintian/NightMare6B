{
  "architecture": "Cybertron",
  "max_seq_len": 512,
  "hidden_dim": 512,
  "n_layers": 8,
  "num_attention_heads": 8,
  "multiple_of": 32,
  "dropout": 0.0,
  "bias": false,
  "intermediate_size": 1024,
  "vocab_size": 152064,
  "rope_scaling_factor": 1.0,
